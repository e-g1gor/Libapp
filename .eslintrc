{
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": 10
  },
  "env": {
    "browser": false,
    "node": true,
    "es6": true
  },
  "rules": {
    "semi": ["error","never"],
    "no-inner-declarations": 2,
    "no-undef": 1,
    "no-unused-vars":"warn",

    "comma-dangle": [
      "error",
      "never"
    ]
  }
}
